lst = [5, 6, 7, 8]
lst.append(6)
print('5,6,7,8,6')
print(lst)

lst.insert(0, 9)
print('9,5,6,7,8,6')
print(lst)

x = lst.pop(2)#6
print('[9,5,7,8,6]')
print(lst)

lst.remove(x)#6
print('Error')
print(lst)

a, b = lst, lst[:]
print('Yes,True')
print(a is lst)#identity?yes

print('--')
print('Yes,True')
print(b == lst)#equailty?yes

print('--')
print('No,False')
print(b is lst)
